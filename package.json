{
  "name": "rubberduckheader",
  "displayName": "Rubberduck Header",
  "description": "RubberduckEngine header for VSCode : www.rubberduckengine.com",
  "version": "1.0.1",
  "publisher": "Fousse",
  "icon": "duck_icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Fousse24/rubberduckheader"
  },
  "bugs": {
    "url": "https://github.com/Fousse24/rubberduckheader/issues"
  },
  "homepage": "https://github.com/Fousse24/rubberduckheader",
  "license": "MIT",
  "engines": {
    "vscode": "^1.6.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "configuration": {
      "properties": {
        "rubberduckheader.username": { 
          "type": "string",
          "description": "Username to use in Rubberduck headers"
        },
        "rubberduckheader.email": { 
          "type": "string",
          "description": "Email address to use in Rubberduck headers"
        }
      }
    },
    "commands": [
      {
        "command": "rubberduckheader.insertHeader",
        "title": "add RubberduckEngine header to the active file",
        "when": "editorTextFocus"
      }
    ],
    "keybindings": [
      {
        "command": "rubberduckheader.insertHeader",
        "key": "ctrl+alt+r",
        "mac": "cmd+alt+r",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/node": "^6.0.51",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "moment": "^2.15.0"
  }
}
